    "start": "node dist/main.js"
  },
  "dependencies": {
    "@pioneer-platform/default-redis": "latest",
    "@bithighlander/loggerdog-client": "^5.8.0",
    "@types/node": "^12.12.44",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "rate-limiter-flexible": "^2.1.7",
    "socket.io": "^2.3.0",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^3.2.1"
  }
}
